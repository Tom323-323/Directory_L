<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/logo_v_5">

    <TextView
        android:id="@+id/textView"
        android:layout_width="389dp"
        android:layout_height="110dp"
        android:layout_marginTop="40dp"
        android:fontFamily="@font/cormorantsc_semibold"
        android:text="@string/logo_name_start"
        android:textAlignment="center"
        android:textColor="@color/colorLogoBack"
        android:textSize="24sp"
        android:typeface="normal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textViewNoAcation2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="175dp"
        android:layout_marginEnd="179dp"
        android:layout_marginBottom="28dp"
        android:fontFamily="@font/cormorantsc_bold"
        android:text="@string/nameTextMain"
        android:textColor="@color/colorLogoBack"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.475"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>